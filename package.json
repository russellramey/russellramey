    "babel-minify": "^0.5.0",
    "babel-preset-env": "^1.7.0",
    "grunt": "^1.5.2",
    "grunt-babel": "^7.0.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-imagemin": "^3.1.0",
    "grunt-contrib-pug": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-sass": "^3.1.0",
